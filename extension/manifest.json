  "version": "0.1",

  "browser_action": {
    "default_title": "Show Rollout",
    "default_icon": "images/icon.png",
    "default_popup": "popup.html"
  },

  "icons": {
    "16": "images/icon.png",
    "19": "images/icon.png",
    "32": "images/icon.png",
    "38": "images/icon.png",
    "48": "images/icon.png",
    "64": "images/icon.png",
    "128": "images/icon.png"
  }
}